<!DOCTYPE html>
<html>
  <head>
    <title>FastMeeting</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  <% if user_signed_in? %>
  <div class="content">
    <div class="contents">
      <%= yield %>
    </div>
  </div>

  <% else %>
    <%= yield %>
  <% end %>
  </body>
</html>
